public class Arrays{
    public static void main (String [] args){
         int arr [] = {10, 20, 30, 40, 50};
        //  int [] arr = {10, 20, 30, 40, 50};
        //  int [] arr = new int [5];
        //  int arr[] = new int [] {10, 20, 30, 40, 50};
         // Enhance for loop
         for(int x : arr){
             System.out.println(x);
         }
    }
}
